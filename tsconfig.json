{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "types": ["node"],
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react",
    "declaration": true,
    "declarationMap": true,
    "noImplicitOverride": true
  }
}
